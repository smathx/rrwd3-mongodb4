              Prefix Verb   URI Pattern                                      Controller#Action
    api_race_results GET    /api/races/:race_id/results(.:format)            api/results#index
                     POST   /api/races/:race_id/results(.:format)            api/results#create
 new_api_race_result GET    /api/races/:race_id/results/new(.:format)        api/results#new
edit_api_race_result GET    /api/races/:race_id/results/:id/edit(.:format)   api/results#edit
     api_race_result GET    /api/races/:race_id/results/:id(.:format)        api/results#show
                     PATCH  /api/races/:race_id/results/:id(.:format)        api/results#update
                     PUT    /api/races/:race_id/results/:id(.:format)        api/results#update
                     DELETE /api/races/:race_id/results/:id(.:format)        api/results#destroy
           api_races GET    /api/races(.:format)                             api/races#index
                     POST   /api/races(.:format)                             api/races#create
        new_api_race GET    /api/races/new(.:format)                         api/races#new
       edit_api_race GET    /api/races/:id/edit(.:format)                    api/races#edit
            api_race GET    /api/races/:id(.:format)                         api/races#show
                     PATCH  /api/races/:id(.:format)                         api/races#update
                     PUT    /api/races/:id(.:format)                         api/races#update
                     DELETE /api/races/:id(.:format)                         api/races#destroy
